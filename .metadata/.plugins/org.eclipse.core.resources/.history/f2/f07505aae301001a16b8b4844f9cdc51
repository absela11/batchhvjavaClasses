
public class Stringcioncation {

	public static void main(String[] args) {
		String s1="Ethio";
		String s2=" ";
		String s3="ABELA";
		System.out.println(s1+s2+s3);
		
		System.out.println("********");
		String b1="Ethio";
		int b2= 28444;
		String b3="ABELA";
		System.out.println(b1+b2+b3);
	}

}
